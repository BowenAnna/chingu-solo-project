{
    "manifest_version": 3,
    "name": "Your Extension",
    "version": "1.0",
    "description": "A description of your extension",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["https://johnmeade-webdev.github.io/chingu_quiz_api/trial.json"],
        "matches": ["<all_urls>"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  